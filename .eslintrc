{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "worker": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": 0,
    "prettier/prettier": "error"
  }
}